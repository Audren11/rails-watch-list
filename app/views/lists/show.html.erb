<h1>Lists#show</h1>
<p>Find me in app/views/lists/show.html.erb</p>
<%= link_to "Add a movie", new_list_bookmark_path(@list), class:"btn btn-flat"%>

 <div class="container">
    <div class="row">
      <div class="col-sm">
        <% @list.bookmarks.each do |bookmark| %>
          <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(https://raw.githubusercontent.com/lewagon/fullstack-images/master/uikit/breakfast.jpg)">
            <%= bookmark.movie.title %>
            <%= bookmark.comment %>
          </div>
        <%end%>
      </div>
   </div>
  </div>
